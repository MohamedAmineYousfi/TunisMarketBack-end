{
    "version": 2,
    "builds": [
        {
            "src": "src/main.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }] ,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.js",
            "methods": [ "GET", "POST", "PUT", "PATCH", "DELETE" ]
        }
    ]
}
